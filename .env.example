# Eminium Launcher Environment Variables

# API Configuration
API_BASE_URL=https://eminium.ovh
API_TIMEOUT=10000

# Authentication
AUTH_CLIENT_ID=dev_client_id_12345
AUTH_CLIENT_SECRET=dev_client_secret_abcdef123456

# Azuriom Authentication
AZURIOM_URL=https://your-azuriom-site.com

# Discord RPC
DISCORD_CLIENT_ID=dev_discord_client_id
DISCORD_BOT_TOKEN=dev_bot_token

# Update Server
UPDATE_SERVER_URL=https://api.github.com/repos/Eminium-Games/EminiumLauncher/releases/latest

# Logging
LOG_LEVEL=info
LOG_MAX_ENTRIES=100

# Security
ENCRYPTION_KEY=eminium_launcher_dev_key_2024_secure

# Development
NODE_ENV=development
DEBUG=true
